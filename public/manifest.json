{
  "name":"todayy-focus",
  "version":"1.0.0",
  "manifest_version":2,
  "permissions":["storage"],
  "icons":{
      "128":"butterfly-128.png"
  },
  "browser_action": {
      "default_popup": "index.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}